#!/bin/zsh

# ESPN Fantasy Football Credentials
# Replace the values below with your actual credentials from ESPN
# To find these:
# 1. Log into ESPN Fantasy Football
# 2. Open browser developer tools (F12)
# 3. Go to Application/Storage > Cookies
# 4. Find and copy the SWID and ESPN_S2 values

export SWID="YOUR_SWID_HERE"  # Example: {XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX}
export ESPN_S2="YOUR_ESPN_S2_HERE"

echo "ESPN Fantasy Football credentials have been set!"
echo "To verify, run: echo \$SWID" 